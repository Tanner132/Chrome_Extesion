{
  "name": "Snack Time",
  "manifest_version":2,
  "version": "1.0",
  "description": "sends a remider to do thing like order food during binge sessions.",
  "content_scripts":[
    {
      "matches": [
       "https://www.netflix.com/"
      ],
      "js":["index.js"]
    }],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
},
  "permissions": [
    "tabs"
  ]
}